{
  "expo": {
    "name": "Simple Pokedex",
    "slug": "SimplePokedex",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "icon": "./assets/blackPokeball.png",
    "splash": {
      "image": "./assets/pokeballSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFE7E0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "com.smallfunall.simplepokedex",
      "versionCode": 1,
      "permissions": [
        "VIBRATE"
      ]
    }
  }
}